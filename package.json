{
  "name": "songdown-app",
  "version": "0.1.5",
  "private": true,
  "dependencies": {
    "babel": "5.8.21",
    "babelify": "6.1.3",
    "browserify": "11.0.1",
    "express-spa-router": "0.0.8",
    "lodash": "3.10.1",
    "react": "0.13.3",
    "react-router": "0.13.3",
    "react-textarea-autosize": "2.5.0",
    "reflux": "0.2.11",
    "songdown-compiler": "0.4.0"
